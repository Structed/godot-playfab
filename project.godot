}

[application]

config/name="godot-playfab"
run/main_scene="res://Scenes/Main.tscn"
boot_splash/image="res://raw_assets/logo.png"
config/icon="res://icon.png"

[autoload]

Global="*res://Scripts/Global.gd"
SceneManager="*res://Scripts/SceneManager.gd"

[editor_plugins]

enabled=PoolStringArray( "res://addons/godot-playfab/plugin.cfg", "res://addons/godot-plugin-refresher/plugin.cfg" )

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
